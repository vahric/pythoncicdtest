pipeline {
    
    agent any
    
    stages {
   
	stage('Checkout SCM') {
 	    steps {
    		checkout([
      			$class: 'GitSCM',
      			branches: [[name: 'master']],
      			userRemoteConfigs: [[
        		url: 'https://github.com/vahric/pythoncicdtest.git',
        		credentialsId: '',
      			]]
     			])
   }
}

       stage('Do the deployment') {
            
            steps {
                echo ">> Run deploy applications "
            }
        }
    }
}
