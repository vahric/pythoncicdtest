pipeline {
    
    agent any
    
    stages {
   
       stage('checkout') { 
            steps {
		checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/vahric/pythoncicdtest']])
		}
	}

       stage('Do the deployment') {
            
            steps {
                echo ">> Run deploy applications "
            }
        }
    }
}
